        script.

          $('.valuesDollBTC').html('BTC/USD $'+parseFloat('#{BTC}').toFixed(2));
          $('.valuesDollETH').html('ETH/USD $'+parseFloat('#{ETH}').toFixed(2));


          if('#{userDetails.accountOnBlockchain}' == 'true'){
            //- $('.generate_address_card').remove();
            $('.generate_address_card').css('display','none');
            getAddr();
          }
          else {
            //- $('.eth_address_card').remove();
            $('.eth_address_card').css('display','none');
          }

          if('#{userDetails.twoFA}' != 'true'){
              $('.2fa_form_validation').remove();
              $("#twoFAEnableModal").modal("show");

          }

          if('#{browserVerified}' =='true')
          {
              $('.sendVerficaionLinkCard').remove();
          }else{
              $('.sendTabCard').remove();
          }


          $('#myModal1').on('hidden.bs.modal', function () {
            $('.textModalHeader').html('Whoops');
            $('.responseText').html('');
          })




          // getting the dollar balance
          get_dollar_Wallet();
          get_all_balance();
          get_current_Investement();
          get_current_value_investement();
          // dollar to sip conversion
          $("#amountinDollar").on("paste keyup", function() {
              $('#sip_value_total_trans').val(($("#amountinDollar").val()/'#{SIP}').toFixed(6));
          });

          // reinvest plane choose

          $('#doll_value_Reinvest').on("paste keyup", function() {
              var dollar_amount=$("#doll_value_Reinvest").val();
              console.log(dollar_amount);
              var sip_amount=(dollar_amount/'#{SIP}').toFixed(6);
              $('#sip_value_Reinvest').val(sip_amount);

              var sipBalance=$('#sip_value_ReinvestHidden').attr('max');
              if(parseFloat(sip_amount)>parseFloat(sipBalance))
              {
                $('.buttonTextChange').html('Insufficient SIPcoins');
                $('.buttonTextChange').css('background-color','grey');
              }else{
                $('.buttonTextChange').html('Reinvest');
                $('.buttonTextChange').css('background-color','#9c27b0');
              }


              if(dollar_amount>= 100 &&  dollar_amount<= 1000)
              {
                $('.planOne').addClass('highlight');
                $('.planFour').removeClass('highlight');
                $('.planThree').removeClass('highlight');
                $('.planTwo').removeClass('highlight');

              }else
              if(dollar_amount>= 1010 &&  dollar_amount<= 10000)
              {
                $('.planTwo').addClass('highlight');
                $('.planFour').removeClass('highlight');
                $('.planThree').removeClass('highlight');
                $('.planOne').removeClass('highlight');


              }else
              if(dollar_amount>= 10010 &&  dollar_amount<= 50000)
              {
                $('.planThree').addClass('highlight');
                $('.planFour').removeClass('highlight');
                $('.planTwo').removeClass('highlight');
                $('.planOne').removeClass('highlight');

              }else
              if(dollar_amount>= 50010 &&  dollar_amount<= 100000)
              {
                $('.planFour').addClass('highlight');
                $('.planThree').removeClass('highlight');
                $('.planTwo').removeClass('highlight');
                $('.planOne').removeClass('highlight');

              }else{
                $('.planFour').removeClass('highlight');
                $('.planThree').removeClass('highlight');
                $('.planTwo').removeClass('highlight');
                $('.planOne').removeClass('highlight');
              }
          });


          function CheckAddress(){

            if('#{userDetails.accountOnBlockchain}' =='true'){
              return true;
            }else{

              $("#myModal").modal("hide");
              $('.responseText').html('Please Create BlockChain Address To Transfer');
              $("#myModal1").modal("show");
              return false;
            }

          }

          function saveplan(){
             var splan = $("input[type='radio'][name='optionsRadios']:checked").val();
             if(splan == undefined || splan == null)
             {
              $.notify("Please Select An Option!");
              return false;
             }
             var now = new Date();
             var oneDay = 24*60*60*1000;
             var enddate ;
             var diffDays ;

             if(splan == "A"){
                enddate = new Date(now.getFullYear()+1, now.getMonth(), now.getDate());
                diffDays = Math.round(Math.abs((now.getTime() - enddate.getTime())/(oneDay)));

              }else{
                enddate = new Date(now.getFullYear()+1, now.getMonth()+6, now.getDate());
                diffDays = Math.round(Math.abs((now.getTime() - enddate.getTime())/(oneDay)));
             }
             //alert("Start date"+ now + "end date "+ enddate +"calculate daye"+diffDays);

              $.ajax({
                     type : 'POST',
                     url : '/savestackingplan',
                     datatype : 'application/json',
                    data : {
                    stakingplane : splan,
                    startdate : now,
                    enddate : enddate,
                    diffDays : diffDays
                    },
                    success: function(response){
                    $.notify("Customer Staking Plan Selected Successfully", "success");
                     window.location.href ="/dashboard";
                    },
                    error: function(){
                      $.notify("Error Selecting Customer Staking Plan");
                    }
             });

          }

          function customerplan(){
           var plan = '#{customerplan}'
           if(plan =="A" || plan =="B"){

               $('#plansubmit').hide();
                if(plan == "A"){
                  $('#optionsRadios1').attr('checked', true);
                  $('#optionsRadios2').attr('disabled', true);
              }else{
                 $('#optionsRadios2').attr('checked', true);
                 $('#optionsRadios1').attr('disabled', true);
              }

           }

          }

          function checkESCROW(){
            var ESCROW = '#{ESCROW}'
            if(!(ESCROW >= 150))
            {
              $('#optionsRadios1').prop('checked', false);
              $('#optionsRadios2').prop('checked', false);

              $.notify("INSUFFICIENT ESCROW FUNDS!");

            }

          }
