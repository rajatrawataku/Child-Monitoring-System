      -webkit-appearance: none;
      -moz-appearance: none;
      appearance: none;
      margin: 0;
      }
      input{
      @include transition(all 0.30s ease-in-out);
      outline: none;
      padding: 6px ;
      margin: 5px 1px 3px 0px;
      border: 1px solid #DDDDDD;
      display: inline;
      border-radius: 5px;
      }
      input:focus{
