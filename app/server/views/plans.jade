    // Header
    header
      .topbar
        .container
          .row
            .col-lg-6.col-md-6.col-sm-6
              .topbar-left
                ul.list-inline
            .col-lg-6.col-md-6.col-sm-6
              .topbar-right.text-right
                ul.list-inline
                  li.list-inline-item
                    ul.list-inline.iq-left
                      li.list-inline-item
                        a(href='/login')
                          i.fa.fa-lock
                          | Login
                      li.list-inline-item
                        a(href='/signup')
                          i.fa.fa-user
