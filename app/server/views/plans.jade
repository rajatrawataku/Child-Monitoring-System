      .iq-header
        .container
          .row
            .col-md-12
              .logo
                a(href='/')
                  img.img-fluid(src='/images/logo/sipcoin_black.png', alt='logo')
              nav
                a#resp-menu.responsive-menu(href='javascript:void(0);')
                  i.fa.fa-reorder
                  |  Menu
                ul.menu.text-right
                  li
                    a(href='/') HOME
                  li
                    a(href='/about') About Us
                  li
