                    a(href='javascript:void(0);') Blog
                  li
                    // <a href="coming-soon.html">Live Trading</a>
                    a(href='javascript:void(0);') Live Trading
                  li
                    a(href='/contact') Contact
                  li
                    .iq-search
                      a.search-btn.not_click(href='javascript:void(0);')
                        span Search Button
                      .search-box.not-click
                        input#search.form-control.not-click(type='text', placeholder='Search')
