      Login
    .modal.fade.iq-login(tabindex='-1', role='dialog', aria-hidden='true')
      .modal-dialog(role='document')
        .modal-content
          .modal-header.text-center
            h4.modal-title.iq-tw-5 Login
            a.close(data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
          .modal-body
            form
              .form-group
                input#recipient-name.form-control(type='text', placeholder='Enter Name')
              .form-group
                input#recipient-password.form-control(type='password', placeholder='Password')
              a.button.iq-mtb-10(href='#', data-toggle='modal', data-target='.iq-login', data-whatever='@mdo') Sign In
              .row
                .col-sm-6
                  .form-check
                    label.form-check-label
                      input.form-check-input(type='checkbox')
                      | Remember Me
                .col-sm-6.text-right
                  a(href='#') Forgot Password
          .modal-footer.text-center
            div
              | Don't Have an Account?
              a.iq-font-yellow(href='#', data-toggle='modal', data-target='.iq-register', data-whatever='@fat') Register Now
