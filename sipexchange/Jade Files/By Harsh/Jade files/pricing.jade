  body
    // loading
    #loading
      #loading-center
        img(src='images/loader.gif', alt='loder')
    // loading End
    // Header
    header
      .topbar
        .container
          .row
            .col-lg-6.col-md-6.col-sm-6
              .topbar-left
                ul.list-inline
            .col-lg-6.col-md-6.col-sm-6
              .topbar-right.text-right
                ul.list-inline
                  li.list-inline-item
                    ul.list-inline.iq-left
                      li.list-inline-item
                        a(href='#', data-toggle='modal', data-target='.iq-login', data-whatever='@mdo')
                          i.fa.fa-lock
                          | Login
                      li.list-inline-item
                        a(href='#', data-toggle='modal', data-target='.iq-register', data-whatever='@fat')
                          i.fa.fa-user
                          | Register
                  li.list-inline-item
                    a(href='#')
                      i.fa.fa-comments-o
                      | Free Consulting
      .iq-header
        .container
          .row
            .col-md-12
              .logo
                a(href='/home-2 ')
                  img.img-fluid(src='images/logo/sipcoin_black.png', alt='logo')
              nav
                a#resp-menu.responsive-menu(href='#')
                  i.fa.fa-reorder
                  |  Menu
                ul.menu.text-right
                  li
                    a(href='/home-2 ') HOME
                  li
                    a(href='/about-us ') About Us
                  li
                    a.active(href='/pricing ') Plans
                  li
                    // <a href="/faqs-1 ">FAQ's</a>
                    a(href='#') FAQ's
                  li
                    // <a href="/blog-left-sidebar ">Blog</a>
                    a(href='#') Blog
                  li
                    // <a href="/coming-soon-1 ">Live Trading</a>
                    a(href='#') Live Trading
                  li
                    a(href='/contact-2 ') Contact
                  li
                    .iq-search
                      a.search-btn.not_click(href='javascript:void(0);')
                        span Search Button
                      .search-box.not-click
                        input#exampleInputEmail1.form-control.not-click(type='email', placeholder='Search')
                        i.ti-search
    // Header End
    .clearfix
    // ======= Breadcrumb Inner Page =======
    section.iq-bg.iq-bg-fixed.iq-over-black-80.jarallax.iq-breadcrumb.text-center.iq-font-white(style="background-image: url('images/bg/plan3.jpg'); background-position: center center;")
